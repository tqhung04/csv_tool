<p id="notice"><%= notice %></p>

<h1>Csv Operators</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_operators.each do |csv_operator| %>
      <tr>
        <td><%= csv_operator.type %></td>
        <td><%= csv_operator.name %></td>
        <td><%= link_to 'Show', csv_operator %></td>
        <td><%= link_to 'Edit', edit_csv_operator_path(csv_operator) %></td>
        <td><%= link_to 'Destroy', csv_operator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Operator', new_csv_operator_path %>
