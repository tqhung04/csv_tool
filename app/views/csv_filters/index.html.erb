<p id="notice"><%= notice %></p>

<h1>Csv Filters</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_filters.each do |csv_filter| %>
      <tr>
        <td><%= csv_filter.name %></td>
        <td><%= csv_filter.type %></td>
        <td><%= link_to 'Show', csv_filter %></td>
        <td><%= link_to 'Edit', edit_csv_filter_path(csv_filter) %></td>
        <td><%= link_to 'Destroy', csv_filter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Filter', new_csv_filter_path %>
