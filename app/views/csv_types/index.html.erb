<p id="notice"><%= notice %></p>

<h1>Csv Types</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Project</th>
      <th>CsvAttribute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_types.each do |csv_type| %>
      <tr>
        <td><%= csv_type.id %></td>
        <td><%= csv_type.name %></td>
        <td><%= csv_type.project_id %></td>
        <td><%= link_to 'CsvAttribute', project_csv_type_csv_attributes_path(csv_type.project, csv_type) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Type', new_project_csv_type_path %>
