<p id="notice"><%= notice %></p>

<h1>Csv Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_types.each do |csv_type| %>
      <tr>
        <td><%= csv_type.name %></td>
        <td><%= csv_type.project_id %></td>
        <td><%= link_to 'Show', csv_type %></td>
        <td><%= link_to 'Edit', edit_csv_type_path(csv_type) %></td>
        <td><%= link_to 'Destroy', csv_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Type', new_csv_type_path %>
